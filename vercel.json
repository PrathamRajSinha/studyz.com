{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/study-pathway",
      "dest": "server/server.js"
    },
    {
      "src": "/initiate-content-generation",
      "dest": "server/server.js"
    },
    {
      "src": "/check-content-status",
      "dest": "server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server/server.js"
    }
  ]
}